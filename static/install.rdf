<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>zotero-wikicite@wikidata.org</em:id>
        <em:name>Cita</em:name>
        <em:description>Citations metadata support for Zotero</em:description>
        <em:version>0.5.2-beta</em:version>
        <em:multiprocessCompatible>true</em:multiprocessCompatible>
        <em:updateURL>https://raw.githubusercontent.com/diegodlh/zotero-cita/master/update-beta.rdf</em:updateURL>

        <em:localized>
            <Description>
                <em:locale>en-US</em:locale>
                <em:name>Cita</em:name>
                <em:description>Citations metadata support for Zotero</em:description>
                <em:creator>Diego de la Hera, Dominic Dall'Osto, and others</em:creator>
                <em:homepageURL>https://github.com/diegodlh/zotero-cita</em:homepageURL>
            </Description>
        </em:localized>

        <em:creator>Diego de la Hera, Dominic Dall'Osto, and others</em:creator>
        <em:homepageURL>https://github.com/diegodlh/zotero-cita</em:homepageURL>
        <em:optionsURL>chrome://cita/content/preferences.html</em:optionsURL>
        <em:type>2</em:type> <!-- type=extension -->
        <em:iconURL>chrome://cita/skin/cita.png</em:iconURL>
        <em:icon64URL>chrome://cita/skin/cita64.png</em:icon64URL>
        <em:bootstrap>true</em:bootstrap>

        <em:translator>es: Diego de la Hera</em:translator>
        
        <!-- Or requires Zotero Standalone -->
        <em:targetApplication>
            <Description>
                <em:id>zotero@chnm.gmu.edu</em:id>
                <em:minVersion>5.0</em:minVersion>
                <em:maxVersion>*</em:maxVersion>
            </Description>
        </em:targetApplication>

        <!-- Or requires Juris-M -->
        <em:targetApplication>
          <Description>
            <em:id>juris-m@juris-m.github.io</em:id>
            <em:minVersion>4.0</em:minVersion>
            <em:maxVersion>*</em:maxVersion>
          </Description>
        </em:targetApplication>


        <em:localized>
            <Description>
                <em:locale>es</em:locale>
                <em:name>Cita</em:name>
                <em:description>Soporte para metadatos de citas en Zotero</em:description>
                <em:creator>Diego de la Hera, Dominic Dall'Osto, y otrxs</em:creator>
                <em:homepageURL>https://github.com/diegodlh/zotero-cita</em:homepageURL>
            </Description>
        </em:localized>
    </Description>
</RDF>
